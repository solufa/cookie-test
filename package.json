{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:client": "cross-env NUXT_PORT=8000 nuxt",
    "dev:server": "npm run dev --prefix server",
    "dev:server2": "npm run dev --prefix server2",
    "dev:aspida": "aspida --watch",
    "dev:css": "tcm pages --watch",
    "dev:pathpida": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "run-p build:*",
    "prebuild:client": "run-p generate:aspida generate:css",
    "build:client": "pathpida --enableStatic --ignorePath .gitignore && nuxt generate",
    "build:server": "npm run build --prefix server",
    "build:server2": "npm run build --prefix server2",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:css": "tcm pages",
    "generate:server": "npm run generate --prefix server",
    "generate:server2": "npm run generate --prefix server2",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js . && prettier --check \"./**/*.{ts,js,vue}\"",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.js . --fix && prettier --write \"./**/*.{ts,js,vue}\" && npm run lint:style -- --fix",
    "start": "run-p start:*",
    "start:client": "nuxt serve",
    "start:server": "npm start --prefix server",
    "start:server2": "npm start --prefix server2",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server && tsc --noEmit -p server2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.33.1",
    "axios": "^0.27.2",
    "nuxt": "^2.15.8",
    "pathpida": "^0.19.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "notios": "^0.4.0",
    "pathpida": "^0.19.3",
    "postcss-html": "^1.3.0",
    "prettier": "^2.8.0",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "ts-loader": "^8.4.0",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.9.3"
  }
}
